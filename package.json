  },
  "devDependencies": {
    "@nice-move/cli": "^0.5.22",
    "@nice-move/commitlint-config": "^0.1.3",
    "@nice-move/eslint-config-base": "^0.5.39",
    "@nice-move/prettier-config": "^0.4.4",
    "ava": "^3.15.0",
    "commitlint": "^12.1.4",
    "eslint": "^7.28.0",
    "eslint-plugin-ava": "^12.0.0",
    "garou": "^0.1.28",
    "prettier": "^2.3.1"
  },
  "peerDependencies": {
